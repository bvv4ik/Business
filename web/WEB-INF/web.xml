    <web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
        metadata-complete="false"
>

         
    <!-- <web-app> -->
    <servlet>
        <servlet-name>showdb</servlet-name>
        <servlet-class>students.web.ShowDB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>students.web.HelloWorldServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>main2</servlet-name>
        <servlet-class>students.web.Main2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>students.web.Login</servlet-class>

    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.bw.service.Login</servlet-class>
           <!--       <init-param>
                   <param-name>log4j</param-name>
                   <param-value>WEB-INF/log4j.xml</param-value>
                   </init-param>  -->
    </servlet>
    <servlet>
        <servlet-name>PlaceServlet</servlet-name>
        <servlet-class>com.bw.service.PlaceServlet</servlet-class>
    </servlet>
    
    
    <servlet-mapping>
        <servlet-name>showdb</servlet-name>
        <url-pattern>/showdb</url-pattern>
        
        <!-- <web-app> -->
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>main2</servlet-name>
        <url-pattern>/main2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>

    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlaceServlet</servlet-name>
        <url-pattern>/PlaceServlet</url-pattern>
    </servlet-mapping>
</web-app>
